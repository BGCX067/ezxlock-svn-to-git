####### Build rules


all: $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

moc: $(SRCMOC)

tmake: Makefile

Makefile: make.pro
	tmake make.pro -o Makefile

dist:
	$(TAR) make.tar make.pro $(SOURCES) $(HEADERS) $(INTERFACES) $(DIST)
	$(GZIP) make.tar

clean:
